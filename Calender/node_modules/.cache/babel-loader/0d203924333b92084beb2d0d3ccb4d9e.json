iv>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    name: 'DCHeros',\r\n     computed: {\r\n    herosCount() {\r\n      return this.heroes.length;\r\n    },\r\n  },\r\n  methods: {\r\n    addHero() {\r\n      if ( this.newHero !== '') {\r\n        this.heroes.push(this.newHero);\r\n        this.newHero = '';\r\n      }\r\n    },\r\n    setFullName() {\r\n      this.fullname = 'Gabriel Ilochi';\r\n    },\r\n    remove(index) {\r\n      this.heroes = this.heroes.filter((hero, i) => i !== index);\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      heroes: [\r\n        'Gabriel',\r\n        'Jake',\r\n        'Bright',\r\n        'Rose',\r\n      ],\r\n      newHero: '',\r\n    };\r\n  },\r\n}\r\n</script>"],"sourceRoot":""}]}