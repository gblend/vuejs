ere\">\n  <button\n  class=\"p-2 m-2border rounded bg-gradient-to-t from-blue-400 to-blue-600\"\n  type=\"submit\">Add Hero</button>\n</form>\n  </div>\n</div>\n</template>\n\n<script>\nimport { AppHeader } from './components/AppHeader';\nexport default {\n  components: {\nAppHeader,\n  },\n  name: 'App',\n  computed: {\n    herosCount() {\n      return this.heroes.length;\n    },\n  },\n  methods: {\n    addHero() {\n      if ( this.newHero !== '') {\n        this.heroes.push(this.newHero);\n        this.newHero = '';\n      }\n    },\n    setFullName() {\n      this.fullname = 'Gabriel Ilochi';\n    },\n    remove(index) {\n      this.heroes = this.heroes.filter((hero, i) => i !== index);\n    },\n  },\n  data() {\n    return {\n      heroes: [\n        'Gabriel',\n        'Jake',\n        'Bright',\n        'Rose',\n      ],\n      newHero: '',\n    };\n  },\n};\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n}\n</style>\n\n"]}]}